{
  "name": "cwp/watea-theme",
  "description": "CWP Watea theme",
  "type": "silverstripe-theme",
  "keywords": ["silverstripe", "theme", "cwp"],
  "license": "BSD-3-Clause",
  "require": {
    "cwp/starter-theme": "dev-i3-modern",
    "cwp/agency-extensions": "^2.0"
  },
  "extra": {
    "installer-name": "watea",
    "branch-alias": {
      "dev-master": "3.x-dev"
    },
    "expose": [
      "dist",
      "ico",
      "images"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/i3waitematadhb/cwp-starter-theme.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
